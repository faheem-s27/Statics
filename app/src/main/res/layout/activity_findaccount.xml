<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ic_launcher_background">


    <TextView
        android:id="@+id/animateName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/valorant"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Loading Duck#2004 stats..."
        tools:visibility="gone" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/sharePlayerProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:alpha="0"
        android:backgroundTint="#eb0029"
        android:clickable="true"
        app:backgroundTint="#EB0029"
        app:fabSize="mini"
        app:layout_constraintBottom_toTopOf="@+id/fabPlus"
        app:layout_constraintEnd_toEndOf="@+id/fabPlus"
        app:layout_constraintStart_toStartOf="@+id/fabPlus"
        app:srcCompat="?attr/actionModeShareDrawable"
        app:tint="@color/white" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/RSOLogOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:alpha="0"
        android:backgroundTint="#eb0029"
        android:clickable="true"
        app:backgroundTint="#EB0029"
        app:fabSize="mini"
        app:layout_constraintBottom_toTopOf="@+id/sharePlayerProfile"
        app:layout_constraintEnd_toEndOf="@+id/fabPlus"
        app:layout_constraintStart_toStartOf="@+id/fabPlus"
        app:srcCompat="@drawable/rsologout"
        app:tint="@color/white" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabPlus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#eb0029"
        android:clickable="true"
        android:foregroundGravity="center"
        android:rotation="45"
        android:visibility="visible"
        app:backgroundTint="@color/ic_launcher_background"
        app:fabSize="auto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:drawable/ic_menu_close_clear_cancel"
        app:tint="@color/white" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/goBackFromLink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:alpha="1"
        android:backgroundTint="#eb0029"
        android:clickable="true"
        android:foregroundGravity="center"
        android:visibility="invisible"
        app:backgroundTint="@color/ic_launcher_background"
        app:fabSize="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="?attr/actionModeCloseDrawable"
        app:tint="@color/white" />

    <ImageView
        android:id="@+id/imagebackground"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.2"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:visibility="visible">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:context=".valo.activities.ValorantMainMenu">

            <androidx.cardview.widget.CardView
                android:id="@+id/circle"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="16dp"
                android:elevation="12dp"
                app:cardCornerRadius="200dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/playerProfile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="fitCenter"
                    android:src="@drawable/valologo"
                    app:srcCompat="@drawable/agentesicon" />

            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/playerLevel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="-20dp"
                android:autoSizeTextType="uniform"
                android:background="@color/ic_launcher_background"
                android:elevation="100dp"
                android:fontFamily="@font/valorant"
                android:padding="5dp"
                android:text="999"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/circle"
                app:layout_constraintEnd_toEndOf="@+id/circle"
                app:layout_constraintStart_toStartOf="@+id/circle"
                app:layout_constraintTop_toBottomOf="@+id/circle" />

            <Button
                android:id="@+id/crosshairBT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:backgroundTint="@color/ic_launcher_background"
                android:clickable="true"
                android:fontFamily="@font/baloo_bhai"
                android:gravity="center"
                android:text="Crosshair Settings"
                android:textColor="#FFFFFF"
                app:backgroundTint="@color/ic_launcher_background"
                app:cornerRadius="50dp"
                app:fabSize="auto"
                app:icon="@drawable/newcrosshair"
                app:iconPadding="8dp"
                app:iconSize="30dp"
                app:iconTint="#FFFFFF"
                app:iconTintMode="src_in"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView14"
                app:srcCompat="@drawable/crosshair"
                app:strokeColor="@color/light_blue_50"
                app:strokeWidth="1dp"
                app:tint="@color/white" />

            <HorizontalScrollView
                android:id="@+id/horizontalScrollView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView8">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/agentsCozBT"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        android:text="Agents"
                        android:textAlignment="center"
                        app:strokeColor="@color/Valorant_Blue"
                        app:strokeWidth="1dp"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                    <Button
                        android:id="@+id/weaponsBT"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        app:strokeColor="@color/Valorant_Red"
                        app:strokeWidth="1dp"
                        android:text="WeApons"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                    <Button
                        android:id="@+id/playerCardsBT"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        android:text="Cards"
                        android:textAlignment="center"
                        app:strokeColor="@color/Valorant_Blue"
                        app:strokeWidth="1dp"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                    <Button
                        android:id="@+id/spraysBT"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        android:text="Sprays"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:strokeColor="@color/Valorant_Red"
                        app:strokeWidth="1dp"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                    <Button
                        android:id="@+id/buddiesBT"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        android:text="Buddies"
                        app:strokeColor="@color/Valorant_Blue"
                        app:strokeWidth="1dp"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                    <Button
                        android:id="@+id/mapsButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        android:text="Maps"
                        app:strokeColor="@color/Valorant_Red"
                        app:strokeWidth="1dp"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                    <Button
                        android:id="@+id/ranksButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        app:strokeColor="@color/Valorant_Blue"
                        app:strokeWidth="1dp"
                        android:text="Ranks"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                    <Button
                        android:id="@+id/levelBordersBT"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        app:strokeColor="@color/Valorant_Red"
                        app:strokeWidth="1dp"
                        android:text="Level Borders"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                    <Button
                        android:id="@+id/titlesBT"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        android:text="Titles"
                        app:strokeColor="@color/Valorant_Blue"
                        app:strokeWidth="1dp"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                    <Button
                        android:id="@+id/seasonsBT"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        app:strokeColor="@color/Valorant_Red"
                        app:strokeWidth="1dp"
                        android:text="Seasons"
                        android:textAlignment="center"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                    <Button
                        android:id="@+id/bundlesBT"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:fontFamily="@font/baloo_bhai"
                        android:text="Bundles"
                        android:textAlignment="center"
                        app:strokeColor="@color/Valorant_Blue"
                        app:strokeWidth="1dp"
                        android:textColor="#FFFFFF"
                        android:textStyle="bold"
                        app:cornerRadius="50dp"
                        app:iconTint="#FFFFFF" />

                </LinearLayout>

            </HorizontalScrollView>

            <TextView
                android:id="@+id/playerNameMenu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/valorant"
                android:text="Getting name..."
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/playerLevel"
                tools:text="SprinkledRainbow#1593" />

            <TextView
                android:id="@+id/rankPatchedMainMenu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/valorant"
                android:text="Getting rank..."
                android:textSize="18dp"
                app:layout_constraintStart_toEndOf="@+id/rankImageMainMenu"
                app:layout_constraintTop_toTopOf="@+id/rankImageMainMenu" />

            <ProgressBar
                android:id="@+id/rankProgressMainMenu"
                style="?android:attr/progressBarStyleHorizontal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="16dp"
                android:indeterminate="false"
                android:max="100"
                android:progress="24"
                android:progressTint="#EB0029"
                android:visibility="invisible"
                app:layout_constraintEnd_toStartOf="@+id/MMRFAB"
                app:layout_constraintStart_toEndOf="@+id/rankImageMainMenu"
                app:layout_constraintTop_toBottomOf="@+id/rankPatchedMainMenu"
                tools:visibility="visible" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/buildTrackerGGProfile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:clickable="true"
                app:backgroundTint="@color/ic_launcher_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/crosshairBT"
                app:srcCompat="@drawable/info"
                app:tint="@color/white"
                app:useCompatPadding="true" />

            <TextView
                android:id="@+id/rankNumberMainMenu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/valorant"
                android:text="24/100"
                android:textAlignment="center"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="@+id/rankProgressMainMenu"
                app:layout_constraintTop_toBottomOf="@+id/rankProgressMainMenu"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/valorant"
                android:text="--- Current Ranking ---"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider6" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/valorant"
                android:text="--- Last Match Played ---"
                android:textAlignment="center"
                android:textAllCaps="false"
                android:textColor="#FFFFFF"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view2" />

            <ImageView
                android:id="@+id/lastMatchMapImage"
                android:layout_width="0dp"
                android:layout_height="125dp"
                android:layout_marginTop="16dp"
                android:alpha="1"
                android:scaleType="centerCrop"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView6"
                tools:srcCompat="@tools:sample/backgrounds/scenic" />

            <View
                android:id="@+id/divider6"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="16dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/playerNameMenu" />

            <View
                android:id="@+id/divider4"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="16dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/liveMatch" />

            <View
                android:id="@+id/divider5"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="16dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/horizontalScrollView" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/valorant"
                android:text="--- Not finished yet ---"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="20dp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider5" />

            <ImageView
                android:id="@+id/rankImageMainMenu"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:visibility="invisible"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3"
                tools:srcCompat="@tools:sample/avatars"
                tools:visibility="visible" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/MMRFAB"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:backgroundTint="@color/ic_launcher_background"
                android:clickable="true"
                android:foregroundGravity="center"
                app:backgroundTint="@color/ic_launcher_background"
                app:fabSize="auto"
                app:layout_constraintBottom_toBottomOf="@+id/rankNumberMainMenu"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3"
                app:srcCompat="@drawable/rankicon"
                app:tint="@color/white" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/RecentMatchFAB"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:backgroundTint="#C8111822"
                android:clickable="true"
                android:foregroundGravity="center"
                android:visibility="visible"
                app:backgroundTint="@color/ic_launcher_background"
                app:fabSize="auto"
                app:layout_constraintStart_toStartOf="@+id/lastMatchMapImage"
                app:layout_constraintTop_toBottomOf="@+id/lastMatchMapImage"
                app:srcCompat="@drawable/recentmatchesicon"
                app:tint="@color/white" />

            <ImageView
                android:id="@+id/agentImageMainMenu"
                android:layout_width="70dp"
                android:layout_height="0dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="24dp"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="@+id/lastMatchMapImage"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/lastMatchMapImage"
                tools:srcCompat="@tools:sample/avatars"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/gameStartMainMenu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:fontFamily="@font/baloo_bhai"
                android:shadowColor="#000000"
                android:shadowDx="1.5"
                android:shadowDy="1.3"
                android:shadowRadius="2.0"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="14dp"
                app:layout_constraintBottom_toBottomOf="@+id/lastMatchMapImage"
                app:layout_constraintEnd_toEndOf="@+id/lastMatchMapImage"
                tools:text="4 hours ago" />

            <TextView
                android:id="@+id/gameModeMainMenu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:fontFamily="@font/baloo_bhai"
                android:shadowColor="#000000"
                android:shadowDx="1.5"
                android:shadowDy="1.3"
                android:shadowRadius="2.0"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="14dp"
                app:layout_constraintEnd_toEndOf="@+id/lastMatchMapImage"
                app:layout_constraintTop_toTopOf="@+id/lastMatchMapImage"
                tools:text="Custom game" />

            <TextView
                android:id="@+id/lastMatchStatsMainMenu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/baloo_bhai"
                android:shadowColor="#000000"
                android:shadowDx="1.5"
                android:shadowDy="1.3"
                android:shadowRadius="2.0"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="25dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/lastMatchMapImage"
                app:layout_constraintEnd_toEndOf="@+id/lastMatchMapImage"
                app:layout_constraintStart_toStartOf="@+id/lastMatchMapImage"
                app:layout_constraintTop_toTopOf="@+id/lastMatchMapImage"
                tools:text="25/18/5" />

            <View
                android:id="@+id/view2"
                style="@style/Divider.Horizontal"
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:layout_marginTop="24dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/rankNumberMainMenu" />

            <SeekBar
                android:id="@+id/howManyMatches"
                style="@android:style/Widget.Material.SeekBar"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:backgroundTint="@color/ic_launcher_background"
                android:max="30"
                android:progress="5"
                android:progressTint="#eb0029"
                android:secondaryProgressTint="@color/ic_launcher_background"
                android:thumbTint="@color/light_blue_50"
                app:layout_constraintBottom_toBottomOf="@+id/RecentMatchFAB"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/RecentMatchFAB" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                android:fontFamily="@font/valorant"
